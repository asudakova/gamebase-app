{
  "printWidth": 110,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 100,
        "tabWidth": 2,
        "singleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": false
      }
    }
  ]
}
